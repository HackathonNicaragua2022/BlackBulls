<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/hgsinfo"
    android:layout_height="match_parent"
    tools:context=".hginfo">
    <!-- Inicio de imagenes-->
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="140dp"
        android:layout_height="126dp"
        app:srcCompat="@drawable/prevencion"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="99dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="140dp"
        android:layout_height="126dp"
        app:srcCompat="@drawable/registrov3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="99dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="140dp"
        android:layout_height="126dp"
        app:srcCompat="@drawable/registrov1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="356dp" />
    <!--    finalizan imagenes-->


<!--    boton de traslado a la pestaña de productos aplicados-->
    <Button
        android:id="@+id/btnpa"
        android:layout_width="175dp"
        android:layout_height="60dp"
        android:backgroundTint="@color/white"
        android:text="Productos aplicados"
        android:textColor="#131313"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="131dp"
        tools:layout_editor_absoluteY="496dp" />
<!--Boton de translado ala pestaña de control de vacunas-->
    <Button
        android:id="@+id/btnce"
        android:layout_width="179dp"
        android:layout_height="59dp"
        android:backgroundTint="@color/white"
        android:text="Control de enfermemedades"
        android:textColor="#131313"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="237dp" />
<!--Boton de traslado al control de enfermedades-->
    <Button
        android:id="@+id/btncv"
        android:layout_width="178dp"
        android:layout_height="59dp"
        android:backgroundTint="@color/white"
        android:text="Control de vacunas"
        android:textColor="#131313"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="237dp" />

</androidx.constraintlayout.widget.ConstraintLayout>