<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lobby1"
    tools:context=".hlobby">
<!--Inicio de imagenes -->
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="140dp"
        android:layout_height="126dp"
        app:srcCompat="@drawable/registrolp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="99dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="140dp"
        android:layout_height="126dp"
        app:srcCompat="@drawable/cow"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="99dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="140dp"
        android:layout_height="126dp"
        app:srcCompat="@drawable/documents"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="333dp" />
<!--Finalzian imagenes -->

<!--    boton de transalado a la pestaña de gestion de informacion-->

    <Button
        android:id="@+id/btngif"
        android:layout_width="175dp"
        android:layout_height="55dp"
        android:text="gestion de informacíon"
        tools:ignore="MissingConstraints"
        android:textColor="#131313"
        android:backgroundTint="@color/white"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="472dp" />
<!--boton de traslado ala pestaña de registro de finca-->
    <Button
        android:id="@+id/btnrf"
        android:layout_width="153dp"
        android:layout_height="59dp"
        android:backgroundTint="@color/white"
        android:text="Registro de finca"
        android:textColor="#131313"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="237dp" />
<!--Boton de translado a la pestaña de identificador de animales-->
    <Button
        android:id="@+id/btnida"
        android:layout_width="162dp"
        android:layout_height="59dp"
        android:backgroundTint="@color/white"
        android:text="identificador de animales"
        android:textColor="#131313"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="234dp"
        tools:layout_editor_absoluteY="237dp" />
</androidx.constraintlayout.widget.ConstraintLayout>